{
  "name": "formula-one-db",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-public-files": "copyfiles -u 1 src/public/*.* src/public/**/*.* dist/",
    "build": "npm run clean && tsc && npm run copy-public-files",
    "prestart": "npm run build",
    "start": "node ./dist/bin/www",
    "dev": "cross-env NODE_ENV=dev nodemon --watch src -e ts,ejs --ignore dist/ --exec \"npx ts-node ./src/bin/www.ts\"",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mssql": "^6.4.1",
    "nodemon": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "typeorm": "^0.3.6"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.0.2",
    "@types/jsdom": "^16.2.13",
    "@types/mongodb": "^3.6.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.9.2",
    "@types/request": "^2.48.7",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "kill-port": "^1.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
